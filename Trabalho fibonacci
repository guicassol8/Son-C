#include "stdio.h"
int main (){
    int a = 0;
    int b = 0;
    while (1){
        printf ("Escreva um numero\n");
        scanf ("%d",&a);
        if (a>=b) printf ("%d\n",a);
        if else (a<b) printf ("%d\n",b);
        printf ("Escreva um numero\n");
        scanf ("%d",&b);
        if (a>=b) printf ("%d\n",a);
        if else (a<b) printf ("%d\n",b);
    }
}
